{
    "name": "storybook-vue",
    "version": "0.1.0",
    "scripts": {
        "serve": "vue-cli-service serve",
        "start": "start-storybook -s ./public",
        "build": "build-storybook -s ./public -o ./dist_storybook"
    },
    "main": "/src/main.js",
    "dependencies": {
        "core-js": "^2.6.5",
        "vue": "^2.6.10",
        "vue-router": "^3.0.3",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@storybook/vue": "^6.5.15",
        "@vue/cli-plugin-babel": "^5.0.8",
        "@vue/cli-plugin-eslint": "^5.0.8",
        "@vue/cli-service": "^5.0.8",
        "@vue/eslint-config-prettier": "^5.0.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.3.0",
        "babel-preset-vue": "^2.0.2",
        "css-loader": "^6.7.3",
        "eslint": "^5.16.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-vue": "^5.0.0",
        "node-sass": "^7.0.0",
        "prettier": "^1.18.2",
        "sass-loader": "^13.2.0",
        "style-loader": "^1.3.0",
        "vue-loader": "^17.0.1",
        "vue-style-loader": "^4.1.3",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/prettier"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}